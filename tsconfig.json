{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "es2015",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "types/expo-env.d.ts",
    "types/nativewind-env.d.ts",
    "app/index.js",
    "nativewind-env.d.ts",
    "expo-env.d.ts"
  ]
}
